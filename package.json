{
  "name": "simple-clear-console",
  "displayName": "simple-clear-console",
  "description": "clear console log",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
		"*"
	],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [{
      "command": "simple-clear-console.clear",
      "title": "clear log"
    }],
		"keybindings": [
			{
				"command": "simple-clear-console.clear",
				"key": "ctrl+f10",
				"mac": "cmd+f10",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "simple-clear-console.clear",
					"group": "navigation"
				}
			]
		}
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}
